<h1>If its free its fo me!</h1>

<h3>Create a user here</h3>

<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password_digest, "Password" %>
  <%= f.text_field :password_digest %>

  <%= f.submit %>
<% end %>