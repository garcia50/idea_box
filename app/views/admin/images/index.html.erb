<h1>Admin Images</h1>
<h4><%= link_to "Add Image", new_admin_image_path %></h4>
<br>
<br>
<h4>Images:</h4>

<% @images.each do |image| %>
  <ul>
    <%= image_tag(image.image_url, size: "200x150")%>
    <br>
    <%= link_to "delete", admin_image_path(image), method: "delete", data: {confirm: "Are you sure you want to delete this image?"} %>
  </ul>
<% end %>


